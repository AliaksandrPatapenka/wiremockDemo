{
  "request": {
    "method": "POST",
    "url": "/checkAuthorization",
    "bodyPatterns": [
      {"matchesJsonPath": "$.[?(@.email)]"},
      {"matchesJsonPath": "$.[?(@.password)]"}
    ]
  },
  "response": {
    "status": 200,
    "body": "{\"status\": \"success\", \"userId\": \"{{userId}}\", \"sessionCookie\": \"{{sessionCookie}}\"}",
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": ["custom-auth-transformer"],
	"delayDistribution": {
			"type": "lognormal",
			"median": 200,
			"sigma": 0.1
		}
  }
}