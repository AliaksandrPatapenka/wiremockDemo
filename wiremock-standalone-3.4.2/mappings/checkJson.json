{
	"request": {
		"method": "POST",
		"urlPath": "/checkJson",
		"bodyPatterns": [
			{"matchesJsonPath": "$.userId"},
			{"matchesJsonPath": "$.userName"}
		]
	},
	"response": {
		"status": 200,
		"body": "{\"userId\": \"$(userId)\", \"userName\": \"$(userName)\", \"randomInteger\": \"$(!RandomInteger)\"}",
		"headers": {
			"Content-Type": "application/json"
		},
		"transformers": ["body-transformer"],
		"delayDistribution": {
			"type": "lognormal",
			"median": 200,
			"sigma": 0.1
		}
	}
}